<h1>Tweets</h1>






<div class="row justify-content-center">
    <div class="col-auto">
      <table class="table table-responsive">
         <tr>
    <th>Id </th>
    <th>Title </th>
      </tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.id%></td>
    <td><%= link_to post.title, post%></td>
    
  </tr>
  <% end %>
      </table>
    </div>
  </div>







